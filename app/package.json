{
  "name": "esp-keyboard-mimic",
  "version": "1.0.0",
  "main": "dist/main.js",
  "build": {
    "appId": "com.esp-keyboard-mimic",
    "files": [
      "dist/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "release"
    }
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        }
      ]
    }
  },
  "scripts": {
    "start": "electron-forge start",
    "dev": "cross-env NODE_ENV=development concurrently \"webpack serve --config webpack.config.js\" \"wait-on http://localhost:8080 && electron-forge start\"",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "package": "yarn build && electron-forge package",
    "make": "yarn build && electron-forge make",
    "clean": "rm -rf dist node_modules"
  },
